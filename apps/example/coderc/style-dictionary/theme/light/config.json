{
  "source": [
    "./coderc/style-dictionary/theme/*.tokens.json",
    "./coderc/style-dictionary/theme/light/*.tokens.json"
  ],
  "platforms": {
    "ts": {
      "transformGroup": "react-native",
      "files": [
        {
          "format": "javascript/es6",
          "destination": "src/shared/lib/__tokens__/light.js"
        },
        {
          "format": "typescript/es6-declarations",
          "destination": "src/shared/lib/__tokens__/light.d.ts"
        }
      ]
    }
  }
}
