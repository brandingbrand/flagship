{
  "name": "@brandingbrand/pirateship",
  "sideEffects": false,
  "version": "6.1.0",
  "description": "Base App for Project using Flagship",
  "license": "MIT",
  "private": true,
  "scripts": {
    "add-keys-android": "node ../../node_modules/@brandingbrand/flagship/dist/add-keys-android-internal.js",
    "add-keys-ios": "node ../../node_modules/@brandingbrand/flagship/dist/add-keys-ios-internal.js",
    "compile-android": "react-native run-android --no-packager",
    "compile-ios": "react-native run-ios --no-packager --simulator=\"iPhone X\"",
    "compile-web": "(test -d 'web' || flagship init web) && cd ./web/ && yarn start",
    "build-ios": "./scripts/buildiOS.sh",
    "init": "node scripts/fixRNLink.js && flagship clean && flagship init native",
    "init:web": "node scripts/fixRNLink.js && flagship clean && flagship init web",
    "prepare": "tsc",
    "start": "node node_modules/react-native/local-cli/cli.js start",
    "tsc:watch": "tsc -w --preserveWatchOutput",
    "lint": "fscodestyle",
    "build-web-demo": "./scripts/buildWebDemo.sh"
  },
  "dependencies": {
    "@babel/runtime": "7.4.5",
    "@brandingbrand/flagship": "^6.1.0",
    "@brandingbrand/fsapp": "^6.1.0",
    "@brandingbrand/fsbazaarvoice": "^6.1.0",
    "@brandingbrand/fscategory": "^6.1.0",
    "@brandingbrand/fscommerce": "^6.1.0",
    "@brandingbrand/fscomponents": "^6.1.0",
    "@brandingbrand/fsengage": "^6.1.0",
    "@brandingbrand/fsi18n": "^6.1.0",
    "@brandingbrand/fsmockdatasources": "^6.1.0",
    "@brandingbrand/fsnetwork": "^6.1.0",
    "@brandingbrand/fsproductdetail": "^6.1.0",
    "@brandingbrand/fsproductindex": "^6.1.0",
    "@brandingbrand/fssalesforce": "^6.1.0",
    "@brandingbrand/fsshopify": "^6.1.0",
    "@brandingbrand/fsweb": "^6.1.0",
    "@brandingbrand/react-native-leanplum": "^2.0.3",
    "@react-native-community/async-storage": "^1.6.1",
    "credit-card-type": "^7.0.0",
    "jsc-android": "241213.x.x",
    "lodash-es": "^4.17.10",
    "react": "16.8.3",
    "react-native": "0.59.10",
    "react-native-cookies": "^3.3.0",
    "react-native-device-info": "^0.29.1",
    "react-native-htmlview": "^0.13.0",
    "react-native-i18n": "^2.0.15",
    "react-native-navigation": "https://github.com/brandingbrand/react-native-navigation#v1",
    "react-native-payments": "^0.7.1",
    "react-native-safe-area": "^0.5.0",
    "react-native-sensitive-info": "~5.4.0",
    "react-native-svg": "^9.3.6",
    "react-native-touch-id": "~4.3.0",
    "react-redux": "^5.0.7",
    "tcomb-form-native": "^0.6.12"
  },
  "flagship": {
    "webEnabled": false,
    "webPath": "/",
    "linkNodeModules": [
      "@brandingbrand/fscategory",
      "@brandingbrand/fscommerce",
      "@brandingbrand/fscomponents",
      "@brandingbrand/fsproductindex",
      "@brandingbrand/fsproductdetail",
      "react-native-sensitive-info",
      "react-native-safe-area",
      "react-native-svg",
      "react-native-payments"
    ],
    "webTitle": "PirateShip"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org/"
  },
  "workspaces": {
    "nohoist": [
      "react-native*",
      "**/fsapp",
      "**/fsapp/react-native*",
      "**/fsapp/react-native*/**",
      "jsc-android",
      "**/fsshopify",
      "**/fsshopify/react-native*",
      "**/fsshopify/react-native*/**",
      "@react-native-community/async-storage"
    ]
  }
}
