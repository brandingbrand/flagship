{
  "name": "@brandingbrand/pirateship",
  "version": "3.0.0",
  "description": "Base App for Project using Flagship",
  "license": "MIT",
  "private": true,
  "scripts": {
    "add-keys-android": "node ../../node_modules/@brandingbrand/flagship/dist/add-keys-android.js",
    "add-keys-ios": "node ../../node_modules/@brandingbrand/flagship/dist/add-keys-ios-internal.js",
    "compile-android": "react-native run-android --no-packager",
    "compile-ios": "react-native run-ios --no-packager --simulator=\"iPhone X\"",
    "compile-web": "(test -d 'web' || npm run init web) && cd ./web/ && yarn start",
    "build-ios": "./scripts/buildiOS.sh",
    "init": "flagship clean && flagship init native",
    "prepare": "tsc",
    "start": "node node_modules/react-native/local-cli/cli.js start",
    "tsc:watch": "tsc -w --preserveWatchOutput",
    "lint": "fscodestyle",
    "build-web-demo": "./scripts/buildWebDemo.sh"
  },
  "dependencies": {
    "@babel/runtime": "7.0.0",
    "@brandingbrand/flagship": "^3.0.0",
    "@brandingbrand/fsapp": "^3.0.0",
    "@brandingbrand/fsbazaarvoice": "^3.0.0",
    "@brandingbrand/fscategory": "^3.0.0",
    "@brandingbrand/fscommerce": "^3.0.0",
    "@brandingbrand/fscomponents": "^3.0.0",
    "@brandingbrand/fsengage": "^3.0.0",
    "@brandingbrand/fsi18n": "^3.0.0",
    "@brandingbrand/fsmockdatasources": "^3.0.0",
    "@brandingbrand/fsnetwork": "^3.0.0",
    "@brandingbrand/fsproductdetail": "^3.0.0",
    "@brandingbrand/fsproductindex": "^3.0.0",
    "@brandingbrand/fssalesforce": "^3.0.0",
    "@brandingbrand/fsshopify": "^3.0.0",
    "@brandingbrand/fsweb": "^3.0.0",
    "@brandingbrand/react-native-leanplum": "^2.0.3",
    "credit-card-type": "^7.0.0",
    "jsc-android": "236355.x.x",
    "lodash-es": "^4.17.10",
    "react": "16.6.3",
    "react-native": "0.57.8",
    "react-native-cookies": "^3.3.0",
    "react-native-device-info": "^0.20.0",
    "react-native-htmlview": "^0.13.0",
    "react-native-i18n": "^2.0.15",
    "react-native-navigation": "1.1.493",
    "react-native-payments": "^0.7.1",
    "react-native-push-notification": "^3.1.1",
    "react-native-safe-area": "^0.5.0",
    "react-native-sensitive-info": "^5.1.0",
    "react-native-svg": "^8.0.0",
    "react-native-touch-id": "^4.0.1",
    "react-redux": "^5.0.7",
    "tcomb-form-native": "^0.6.12"
  },
  "flagship": {
    "webEnabled": false,
    "webPath": "/",
    "linkNodeModules": [
      "@brandingbrand/fscategory",
      "@brandingbrand/fscommerce",
      "@brandingbrand/fscomponents",
      "@brandingbrand/fsproductindex",
      "@brandingbrand/fsproductdetail",
      "react-native-sensitive-info",
      "react-native-safe-area",
      "react-native-push-notification",
      "react-native-svg",
      "react-native-payments"
    ],
    "webTitle": "PirateShip"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org/"
  },
  "workspaces": {
    "nohoist": [
      "react-native*",
      "**/fsapp",
      "**/fsapp/react-native*",
      "**/fsapp/react-native*/**",
      "jsc-android",
      "**/fsshopify",
      "**/fsshopify/react-native*",
      "**/fsshopify/react-native*/**"
    ]
  }
}
