{
  "name": "@brandingbrand/pirateship",
  "sideEffects": false,
  "version": "10.2.2",
  "description": "Base App for Project using Flagship",
  "license": "MIT",
  "private": true,
  "scripts": {
    "add-keys-android": "node ../../node_modules/@brandingbrand/flagship/dist/add-keys-android-internal.js",
    "add-keys-ios": "node ../../node_modules/@brandingbrand/flagship/dist/add-keys-ios-internal.js",
    "compile-android": "react-native run-android --no-packager",
    "compile-ios": "react-native run-ios --no-packager --simulator=\"iPhone X\"",
    "compile-web": "(test -d 'web' || flagship init web) && cd ./web/ && yarn start",
    "build-ios": "./scripts/buildiOS.sh",
    "init": "node scripts/fixRNLink.js && flagship clean && flagship init native && yarn init:android-jetify",
    "init:web": "node scripts/fixRNLink.js && flagship clean && flagship init web",
    "init:android-jetify": "jetify",
    "init:android-jetify-reverse": "jetify -r",
    "start:dev-proxy": "cd web && yarn server-dev",
    "start:server-dev": "run-p compile-web start:dev-proxy",
    "start:server-prod": "cd web && yarn server-prod",
    "start:server-ssr": "cd web && yarn server-ssr",
    "prepare": "tsc",
    "start": "node node_modules/react-native/local-cli/cli.js start",
    "tsc:watch": "tsc -w --preserveWatchOutput",
    "lint": "fscodestyle",
    "build-web-demo": "./scripts/buildWebDemo.sh",
    "build-web": "./scripts/buildWeb.sh",
    "build-ssr": "cd web/ && node --max_old_space_size=4096 ./node_modules/.bin/webpack --config ./webpack.ssr.js"
  },
  "dependencies": {
    "@brandingbrand/flagship": "^10.2.2",
    "@brandingbrand/fsapp": "^10.2.2",
    "@brandingbrand/fscategory": "^10.2.2",
    "@brandingbrand/fscommerce": "^10.2.2",
    "@brandingbrand/fscomponents": "^10.2.2",
    "@brandingbrand/fsengage": "^10.2.2",
    "@brandingbrand/fsengagement": "^10.2.2",
    "@brandingbrand/fsi18n": "^10.2.2",
    "@brandingbrand/fsmockdatasources": "^10.2.2",
    "@brandingbrand/fsnetwork": "^10.2.2",
    "@brandingbrand/fsproductdetail": "^10.2.2",
    "@brandingbrand/fsproductindex": "^10.2.2",
    "@brandingbrand/fsweb": "^10.2.2",
    "@brandingbrand/react-native-leanplum": "^5.0.0",
    "@brandingbrand/react-native-payments": "^0.10.0",
    "@brandingbrand/tcomb-form-native": "^0.6.20",
    "@react-native-community/async-storage": "^1.9.0",
    "@react-native-community/cli-platform-android": "^4.7.0",
    "@react-native-community/cli-platform-ios": "^4.7.0",
    "@react-native-community/push-notification-ios": "^1.0.0",
    "credit-card-type": "^8.3.0",
    "jsc-android": "241213.x.x",
    "lodash-es": "^4.17.10",
    "react": "^16.13.1",
    "react-native": "^0.63.2",
    "react-native-cookies": "^3.3.0",
    "react-native-device-info": "^5.0.0",
    "react-native-htmlview": "^0.15.0",
    "react-native-localize": "^1.3.0",
    "react-native-navigation": "^6.0.0",
    "react-native-safe-area": "^0.5.0",
    "react-native-sensitive-info": "^5.5.0",
    "react-native-svg": "^10.0.0",
    "react-native-touch-id": "~4.4.0",
    "react-native-video": "^5.0.0",
    "react-native-webview": "^10.9.0",
    "react-redux": "^7.0.0"
  },
  "flagship": {
    "webEnabled": false,
    "webPath": "/",
    "linkNodeModules": [
      "@brandingbrand/fscategory",
      "@brandingbrand/fscommerce",
      "@brandingbrand/fscomponents",
      "@brandingbrand/fsproductindex",
      "@brandingbrand/fsproductdetail",
      "react-native-sensitive-info",
      "react-native-safe-area",
      "react-native-svg",
      "react-native-payments"
    ],
    "webTitle": "PirateShip"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org/"
  },
  "workspaces": {
    "nohoist": [
      "react-native*",
      "**/fsapp",
      "**/fsapp/react-native*",
      "**/fsapp/react-native*/**",
      "jsc-android",
      "@react-native-community/async-storage",
      "@react-native-community/cli-platform-android",
      "@react-native-community/cli-platform-ios",
      "@react-native-community/push-notification-ios",
      "@brandingbrand/react-native-payments"
    ]
  }
}
