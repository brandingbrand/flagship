{
  "name": "@brandingbrand/kernel-cli",
  "version": "0.0.1",
  "description": "",
  "publishConfig": {
    "access": "public"
  },
  "main": "dist/index.js",
  "module": "dist/index.module.js",
  "types": "dist/index.d.ts",
  "source": "src/index.ts",
  "sideEffects": false,
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    }
  },
  "files": [
    "bin",
    "dist"
  ],
  "bin": {
    "kernel": "bin/kernel.js"
  },
  "scripts": {
    "build": "microbundle --target node --compress --format cjs,esm --tsconfig tsconfig.build.json --no-sourcemap",
    "postbuild": "cp -r src/template dist",
    "dev": "microbundle watch --target node --compress --format cjs,esm --tsconfig tsconfig.build.json"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "commander": "^9.4.1"
  },
  "devDependencies": {
    "@types/node": "^18.11.9"
  }
}
