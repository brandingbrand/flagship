{
  "name": "@brandingbrand/kernel-core",
  "version": "0.0.1",
  "description": "",
  "publishConfig": {
    "access": "public"
  },
  "main": "dist/index.js",
  "module": "dist/index.module.js",
  "types": "dist/index.d.ts",
  "source": "src/index.ts",
  "sideEffects": false,
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "microbundle --target node --compress --format cjs,esm --tsconfig tsconfig.build.json --no-sourcemap",
    "postbuild": "cp -r src/template dist",
    "dev": "microbundle watch --target node --compress --format cjs,esm --tsconfig tsconfig.build.json"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "cli-spinners": "^2.7.0",
    "ejs": "^3.1.8",
    "fastq": "^1.13.0",
    "fs-extra": "^10.1.0",
    "glob": "^8.0.3",
    "log-update": "^4.0.0",
    "rimraf": "^2.7.1"
  },
  "devDependencies": {
    "@types/ejs": "^3.1.1",
    "@types/fs-extra": "^9.0.13",
    "@types/glob": "^8.0.0",
    "@types/rimraf": "^3.0.2"
  }
}
